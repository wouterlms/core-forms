{
  "name": "@wouterlms/forms",
  "version": "0.1.8",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "test": "vitest --dom --global",
    "coverage": "vitest --coverage",
    "publish-lib": "npm run build && cp package.json dist && cd dist && rm vite.config.d.ts && npm publish && cd .. && rm -r dist"
  },
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    }
  },
  "types": "./types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@wouterlms/composables": "^0.1.8",
    "vue": "^3.2.0",
    "vue-i18n": "^9.2.0-beta.30"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/tailwindcss": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "c8": "^7.11.0",
    "eslint": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-putout": "^12.9.1",
    "eslint-plugin-vue": "^8.4.0",
    "happy-dom": "^2.31.1",
    "putout": "^23.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vitest": "^0.2.7",
    "vue-tsc": "^0.29.8"
  }
}
