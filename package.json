{
  "name": "@wouterlms/forms",
  "version": "0.2.06",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "test": "vitest --dom --global",
    "coverage": "vitest --coverage",
    "publish-lib": "npm run build && cp package.json dist && cd dist && rm vite.config.d.ts && npm publish && cd .. && rm -r dist"
  },
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    }
  },
  "types": "./types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-i18n": "^9.1.9"
  },
  "dependencies": {
    "@wouterlms/composables": "^0.1.8"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/tailwindcss": "^3.0.3",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/cli-service": "^4.5.13",
    "c8": "^7.11.0",
    "eslint": "^7.32.0",
    "eslint-config-appwise": "^1.0.2",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-putout": "^14.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "happy-dom": "^2.31.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.13",
    "vitest": "^0.2.7",
    "vue-tsc": "^0.29.8"
  }
}
